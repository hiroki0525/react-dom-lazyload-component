{
  "name": "react-dom-lazyload-component",
  "version": "4.0.0",
  "description": "React Lazy Load Component for your Website Performance",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "install-local-pkg": "node --experimental-json-modules scripts/install-local-package.mjs",
    "uninstall-local-pkg": "node --experimental-json-modules scripts/uninstall-local-package.mjs",
    "start:examples": "npm run -w examples start",
    "start:examples:local": "./scripts/start-examples.sh",
    "start:examples:ci": "./scripts/start-examples.sh ci",
    "predeploy:examples": "npm run install-local-pkg",
    "deploy:examples": "npm run -w examples deploy",
    "postdeploy:examples": "npm run uninstall-local-pkg",
    "build": "npm run clean && rollup -c",
    "test": "jest --runInBand",
    "posttest": "npm run uninstall-local-pkg",
    "lint": "eslint src/",
    "prettier": "prettier src/",
    "size": "size-limit",
    "clean": "rm -rf dist"
  },
  "workspaces": [
    "examples"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiroki0525/react-dom-lazyload-component.git"
  },
  "keywords": [
    "React",
    "JavaScript",
    "TypeScript",
    "IntersectionObserver",
    "Core Web Vitals"
  ],
  "author": "Hiroki Miyaji",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiroki0525/react-dom-lazyload-component/issues"
  },
  "homepage": "https://github.com/hiroki0525/react-dom-lazyload-component#readme",
  "devDependencies": {
    "@babel/preset-env": "7.19.4",
    "@babel/preset-react": "7.18.6",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@rollup/plugin-typescript": "9.0.2",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.6",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@size-limit/preset-small-lib": "8.1.0",
    "@size-limit/time": "8.1.0",
    "@swc/core": "1.3.9",
    "@testing-library/react": "13.4.0",
    "@types/expect-puppeteer": "5.0.1",
    "@types/jest": "29.2.0",
    "@types/jest-environment-puppeteer": "5.0.2",
    "@types/node": "18.11.2",
    "@types/puppeteer": "5.4.7",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "babel-jest": "29.2.2",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.1",
    "jest": "29.2.1",
    "jest-environment-jsdom": "29.2.2",
    "jest-puppeteer": "6.1.1",
    "prettier": "2.7.1",
    "puppeteer": "19.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "rollup": "3.2.3",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-swc3": "0.7.0",
    "semantic-release": "19.0.5",
    "size-limit": "8.1.0",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git",
      "@semantic-release/release-notes-generator"
    ],
    "branches": [
      "master"
    ]
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "4500 ms"
    }
  ]
}
