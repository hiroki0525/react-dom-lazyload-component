{
  "name": "react-dom-lazyload-component",
  "version": "0.0.3",
  "description": "React Lazy Load Component for your Website Performance",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "install:local": "node scripts/install-local-package.mjs",
    "start:examples": "npm run install:local && npm i -w examples && npm run -w examples start",
    "build:examples": "npm run -w examples build",
    "deploy:examples": "npm run -w examples deploy",
    "build": "npm run clean && tsc && cp package.json README.md ./dist",
    "test": "jest",
    "lint": "eslint src/",
    "prttier": "prttier src/",
    "clean": "rm -rf dist",
    "deploy": "npm run build && npm publish --access=public"
  },
  "workspaces": [
    "examples"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiroki0525/react-dom-lazyload-component.git"
  },
  "keywords": [
    "React", "JavaScript", "TypeScript"
  ],
  "author": "Hiroki Miyaji",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiroki0525/react-dom-lazyload-component/issues"
  },
  "homepage": "https://github.com/hiroki0525/react-dom-lazyload-component#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.12.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-test-renderer": "^18.0.0",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
