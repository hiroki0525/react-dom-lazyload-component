{
  "name": "react-dom-lazyload-component",
  "version": "2.1.0",
  "description": "React Lazy Load Component for your Website Performance",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "install-local-pkg": "node --experimental-json-modules scripts/install-local-package.mjs",
    "uninstall-local-pkg": "node --experimental-json-modules scripts/uninstall-local-package.mjs",
    "start:examples": "npm run -w examples start",
    "start:examples:local": "./scripts/start-examples.sh",
    "start:examples:ci": "./scripts/start-examples.sh ci",
    "predeploy:examples": "npm run install-local-pkg",
    "deploy:examples": "npm run -w examples deploy",
    "postdeploy:examples": "npm run uninstall-local-pkg",
    "build": "npm run clean && tsc",
    "test": "jest --runInBand",
    "posttest": "npm run uninstall-local-pkg",
    "lint": "eslint src/",
    "prettier": "prettier src/",
    "clean": "rm -rf dist"
  },
  "workspaces": [
    "examples"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiroki0525/react-dom-lazyload-component.git"
  },
  "keywords": [
    "React",
    "JavaScript",
    "TypeScript"
  ],
  "author": "Hiroki Miyaji",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiroki0525/react-dom-lazyload-component/issues"
  },
  "homepage": "https://github.com/hiroki0525/react-dom-lazyload-component#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@testing-library/react": "^13.2.0",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/node": "^17.0.33",
    "@types/jest": "^27.5.0",
    "@types/jest-environment-puppeteer": "^5.0.2",
    "@types/puppeteer": "^5.4.6",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "babel-jest": "^28.0.3",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.0.2",
    "jest-puppeteer": "^6.1.0",
    "prettier": "^2.6.2",
    "puppeteer": "^13.7.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-test-renderer": "^18.1.0",
    "semantic-release": "^19.0.2",
    "ts-jest": "^28.0.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git",
      "@semantic-release/release-notes-generator"
    ],
    "branches": [
      "main"
    ]
  }
}
