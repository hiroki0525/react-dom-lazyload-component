{
  "name": "react-dom-lazyload-component",
  "version": "4.1.5",
  "description": "React Lazy Load Component for your Website PerformanceðŸš€",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "exports": {
    "development": {
      "require": "./dist/cjs/index.development.js",
      "default": "./dist/index.development.js"
    },
    "production": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "install-local-pkg": "node --experimental-json-modules scripts/install-local-package.mjs",
    "uninstall-local-pkg": "node --experimental-json-modules scripts/uninstall-local-package.mjs",
    "start:examples": "npm run -w examples start",
    "start:examples:local": "./scripts/start-examples.sh",
    "start:examples:ci": "./scripts/start-examples.sh ci",
    "predeploy:examples": "npm run install-local-pkg",
    "deploy:examples": "npm run -w examples deploy",
    "postdeploy:examples": "npm run uninstall-local-pkg",
    "prebuild": "rm -rf dist",
    "build": "rollup -c",
    "postbuild": "find dist | grep -E '^dist/.+/index\\.d\\.ts(\\.map)?$' | xargs rm -rf",
    "test": "jest --runInBand",
    "posttest": "npm run uninstall-local-pkg",
    "lint": "eslint src/",
    "prettier": "prettier src/"
  },
  "workspaces": [
    "examples"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiroki0525/react-dom-lazyload-component.git"
  },
  "keywords": [
    "React",
    "JavaScript",
    "TypeScript",
    "IntersectionObserver",
    "Core Web Vitals"
  ],
  "author": "Hiroki Miyaji",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiroki0525/react-dom-lazyload-component/issues"
  },
  "homepage": "https://github.com/hiroki0525/react-dom-lazyload-component#readme",
  "devDependencies": {
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@rollup/plugin-terser": "^0.2.0",
    "@rollup/plugin-typescript": "10.0.1",
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.7",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@testing-library/react": "13.4.0",
    "@types/expect-puppeteer": "5.0.2",
    "@types/jest": "29.2.5",
    "@types/jest-environment-puppeteer": "5.0.3",
    "@types/node": "18.11.18",
    "@types/puppeteer": "5.4.7",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "babel-jest": "29.3.1",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "3.1.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-puppeteer": "6.2.0",
    "prettier": "2.8.1",
    "puppeteer": "19.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "rollup": "3.9.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "semantic-release": "19.0.5",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.2",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git",
      "@semantic-release/release-notes-generator"
    ],
    "branches": [
      "master"
    ]
  },
  "bundlemon": {
    "baseDir": "./dist",
    "reportOutput": [
      "github"
    ],
    "files": [
      {
        "path": "cjs/index.js"
      },
      {
        "path": "umd/index.js"
      },
      {
        "path": "index.js"
      }
    ]
  }
}
