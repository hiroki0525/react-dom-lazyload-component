{
  "name": "react-dom-lazyload-component",
  "version": "1.0.0",
  "description": "React Lazy Load Component for your Website Performance",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "install:local": "node scripts/install-local-package.mjs",
    "start:examples": "npm run install:local && npm i -w examples && npm run -w examples start",
    "build:examples": "npm run -w examples build",
    "deploy:examples": "npm run -w examples deploy",
    "build": "npm run clean && tsc",
    "test": "jest",
    "lint": "eslint src/",
    "prettier": "prettier src/",
    "clean": "rm -rf dist",
    "deploy": "npm run build && npm publish --access=public"
  },
  "workspaces": [
    "examples"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiroki0525/react-dom-lazyload-component.git"
  },
  "keywords": [
    "React",
    "JavaScript",
    "TypeScript"
  ],
  "author": "Hiroki Miyaji",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiroki0525/react-dom-lazyload-component/issues"
  },
  "homepage": "https://github.com/hiroki0525/react-dom-lazyload-component#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@testing-library/react": "^13.1.1",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-test-renderer": "^18.0.0",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/npm"
    ],
    "branches": [
      "main"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
